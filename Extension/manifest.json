{
    "manifest_version": 3,
    "name": "WebSafe",
    "version": "1.0",
    "description": "Detect phishing websites in real-time.",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}